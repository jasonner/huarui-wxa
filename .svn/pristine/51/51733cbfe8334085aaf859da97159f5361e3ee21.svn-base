/**index.wxss**/
.container{
  width: 100%;
  height: 100%;
  background: #fef8e3;
}
.panic{
  position: absolute;
  right: 92rpx;
  bottom: 75rpx;
  display: block;
  font-size: 36rpx;
  color: #ffaf3e;
  border: 2rpx solid #ffaf3e;
  padding: 16rpx 30rpx;
  background: #fff;
  border-radius: 10rpx;
}
.img{
  width: 100%;
  height: 100%;
  background: #fff;
  display: block;
  background-size: 100% 100%;
  background-repeat:repeat;
}
.iPhoneX{
  width: 100%;
  height: 88%;
  margin-top: 153rpx;
  background: #fff;
  display: block;
  background-size: 100% 100%;
  background-repeat:repeat;
}
.swiperBox{
  position: absolute;
  bottom:80rpx;
  left: 0rpx;
  width: 100%;
  height: 400rpx;
  background: transparent;
  padding-bottom: 10rpx;
}

.slide-image{
  width: 100%;
  height: 370rpx;
  margin: 0rpx auto; 
  border: none;
  margin-bottom: 10rpx;
  padding-bottom: 40rpx;
}
.slide-imageGroup{
  width: 390rpx;
  height:316rpx; 
  position: absolute;
  left: 30rpx;
  top: 21rpx;
}

swiper-container{  
  position: relative;  
}  

.swiper-container .swiper .img{  
  width: 100%;  
  height: 100%;  
}  
.swiper-container .dots{  
  position: absolute;  
  left: 0;  
  right: 0;  
  bottom:50rpx;  
  display: flex;  
  justify-content: center;  
}  
.swiper-container .dots .dot{  
  margin: 0 8rpx;
  width:18rpx;  
  height: 18rpx;  
  background: #999;  
  border-radius: 100%;  
  transition: all .6s;  
}  
.swiper-container .dots .dot.active{  
  width: 18rpx;  
  background: #FDAE4A;  
}  
.tabTextBox{
  position:absolute;
  top:50rpx;
  right:44rpx;
  display:flex;
  flex-direction:column;
  font-size:30rpx;
  text-align: center;
}
.tabTextBox text{
  width: 350rpx;
  flex: 1;
  line-height: 50rpx;
  text-align: center;
}
.tabTextBox text:nth-of-type(1){
  margin-top:10rpx;
  font-size: 34rpx;
  color: #333333;
  letter-spacing: 0.42rpx;
  font-family: PingFangSC-Regular;
  margin-left: 60rpx;
  text-align: center;
  overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.tabTextBox text:nth-of-type(2){
  width:310rpx;
  text-align: left;
  margin-left: 60rpx;
  padding-left: 40rpx;
  box-sizing: border-box;
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #999999;
  letter-spacing: 0.32rpx;
}
.tabTextBox text:nth-of-type(3){
  margin-top:10rpx;
  color: #999;
}
.tabTextBox text:nth-of-type(4){
  width:194rpx;
  height:63rpx;
  border:1rpx solid #FDAE4A;
  text-align: center;
  line-height:63rpx;  
  border-radius: 10rpx; 
  margin-left: 150rpx;
  margin-top: 20rpx;
  font-family: PingFangSC-Regular;
  font-size: 34rpx;
  color: #FDAE4A;
  letter-spacing: 0.3rpx;
}