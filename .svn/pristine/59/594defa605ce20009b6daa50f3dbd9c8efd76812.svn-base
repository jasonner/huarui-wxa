}

.wx-imgBg{
  height: 398rpx;
  width: 100%;
}
.vx-textDetail{
position:absolute;
right:20px;
bottom:14px;
color:#fff;
font-size:28rpx;
}
.wx-listText{
  line-height: 43rpx;
}
.listBox{
  text-align: left;
  display: flex;
  flex-direction: column;
  padding: 30rpx;
  background: #fff; 
  font-size:26rpx; 
  box-sizing: border-box;
  box-shadow: 2rpx 2rpx 2rpx #dfdfdf;
}
.txtf{
  font-size: 24rpx;
  margin-left: -10rpx;
}
.more{
  color:#4292cd;
  font-size: 26rpx;
}
.stateListBox{
  display: flex;
  flex-direction: row;
  flex-wrap:wrap;
  padding-left: 5px;
  padding-right: 5px;
}
.checkbox{
  width: 30rpx;
  height: 30rpx;
}
.stateList{
  flex: 1;
  flex-basis:auto;
  text-align: center;
  background: #fdfbee;
  margin:  0 5rpx;
  margin-top: 50rpx;
  line-height: 60rpx;
}
.active{
  background: #fff;
  position: relative;
  padding-top: 15rpx;
}
.active>.stateListMon{
  color: #E24F50;
  margin-top: -5rpx;
}
.active::before{
    width: 100%;
    height: 3px;
    background: red;
    content: "";
    position: absolute;
    left: 0;
    top: -1px;
    box-sizing: border-box;
}
.stateListMon{
  margin-top: 10px;
  color:#f3b256;
  font-size: 40rpx;
}
.stateListTX{
  font-size: 28rpx;
}
.stateListMun{
  color: #999;
  font-size: 25rpx;
}
