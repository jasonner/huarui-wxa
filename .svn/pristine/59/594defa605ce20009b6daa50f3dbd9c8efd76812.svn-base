}
/* 份数input */
.fewNum{
flex: 1;
width: 100%;
height:104rpx;
line-height: 104rpx; 
display: inline-block;
text-align: right;
padding-right: 30rpx;
z-index:0;
}
/*intrduceBox*/

.intrduceBox{
  padding: 30rpx;
  margin-bottom: 119rpx;
  font-size: 28rpx;
  line-height: 50rpx;
}
checkbox .wx-checkbox-input{
  width: 28rpx;
  height: 28rpx;
  margin-right: 0;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  font-size: 30rpx;
}
/*kefuBox*/
.kefuBox{
  width: 100%;
  background: #fff;
  color: #d9544f;
  display: flex;
  position: fixed;
  bottom: 0;
  left: 0;
}
.kefuBox>view:nth-of-type(1){
font-size: 42rpx;
line-height: 94rpx;
flex: 1;
text-align: center;
}
.kefuBox>view:nth-of-type(2){
  line-height: 94rpx;
  font-size: 34rpx;
  color: #fff;
  background: #f3b256;
  flex:1;
  text-align: center;
}
.kefuText{
  display: block;
  color: #333;
  text-align: center;
  font-size: 14px;
}
.money{
  color: #333;
  font-size:30rpx;
  margin-left: 18rpx; 
}
.noChose{
  color: #e5ae6d;
}

.tab{  
  display: flex;  
  flex-direction: row; 
  z-index:99 
}  
.tab-left{  
  width: 200rpx;  
  line-height: 160%;  
  border-right: solid 1px gray;  
}  
.tab-left view{  
  border-bottom: solid 1px red;  
}  
.tab-left .active{  
  color: #f00;  
}  
.tab-right{  
  line-height: 160%;  
}  
.tab-right .right-item{  
  padding-left: 15rpx;  
  display: none;  
}  
.tab-right .right-item.numchose{  
  display: block;  
}
/*手机号登录  */
.phoneLeft{
  flex: 1;
}
.phoneRight{
  flex: 2.3;
}
.phoneRight>input{
  display: block;
  line-height: 104rpx;
  height:104rpx;
  padding-left: 40rpx;
  z-index: -1;
}
/* 手机验证码 */
.phoneVerificationCodeRight{
  margin-left: 70rpx;
  padding-left: 15rpx;
}
.phoneVerificationCode>text{
  padding:16rpx;
  border: 1px solid  #f3b256;
  border-radius: 10rpx;
  color: #f3b256;
}
.phoneVerificationCodeImg{
  margin-left: 60rpx;
}
/* 服务条款 */
.clauseShow{
  position: fixed;
  width: 95%;
  bottom: 0rpx;
  left: 2%;
  border-radius: 30rpx;
  z-index: 100;
}
.clauseShow .clause{
  width: 100%;
  background: #fff;
  line-height: 60rpx;
  text-align: center;
  font-size: 30rpx;
  border-bottom: 1px solid #dfdfdf;
}
.clauseShow .clause:nth-last-of-type(1){
  margin-top: 10rpx;
  border-radius: 30rpx;
}
.clauseShow .clause navigator{
  font-size: 30rpx;
}
.clauseShow .clause button{
  font-size: 30rpx;
  background: #f3b256;
  color: #fff;
}
/* 手机验证登录 */
.checkLogin button{
  width: 100%;
  background: #e5ae6d;
  color: #fff;
}
/* 不能投保的颜色 */
.kefuBox #grayColor{
  background: #bfbfbf;
}