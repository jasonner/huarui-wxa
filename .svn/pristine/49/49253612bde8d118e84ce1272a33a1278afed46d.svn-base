            relationToAppnt: arr,
            insuredDisable:true
          })

        } else {
          that.setData({
            relationToAppnt: arr,
            insuredDisable: false
          })
        }
      }else{
    
        for (var i = 0; i < insureInfoDetail.relationToAppnt.length;i++){
            arr.push(insureInfoDetail.relationToAppnt[i].text);
        }

        that.setData({
          relationToAppnt:arr,
          insuredDisable:false
        }) 

      }
        
    }
     
   
