<view class='containerBox'>
  <view class="content">
    <view id='payResult'>
        <image id='resultImg' src="img/suc.png" lazy-load="true" mode="widthFix"></image>
        <view>获得一份</view>
        <view class="productName">儿童及成人长期重疾险</view>
    </view>

    <view id='payInfo'>
        <image class='img' src='img/msgBG.png' style='width:100%'></image>
        <view class="itemlist">
          <view class='item-content'>投保单号：<text>4524146541354</text></view>
          <view class='item-content'>被保险人：<text>张瑞</text></view>
          <view class='item-content'>保险期间：<text>20年</text></view>
          <view class='item-content'>支付金额：<text>37.00元</text></view>
        </view>
    </view>


    <button id="confirm" type="primary" size="default" loading="{{false}}" plain="{{false}}"
          disabled="{{false}}" bindtap="primary">点击查看电子保单 </button>
  </view>


</view>